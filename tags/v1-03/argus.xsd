	<xs:element name="ARGUSFrameworkDefinition">
		<xs:annotation>
			<xs:documentation>Description of a ARGUS framework.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Experiment" type="ExperimentDesc">
					<xs:annotation>
						<xs:documentation>Describes the experiment and the framework.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" name="Author" type="AuthorDesc">
					<xs:annotation>
						<xs:documentation>Describes the author of the framework.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Folders">
					<xs:annotation>
						<xs:documentation>Description of all folders of the framework.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element maxOccurs="unbounded" name="Folder" type="FolderDesc">
								<xs:annotation>
									<xs:documentation>Describes a folder of the framework.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Tabs">
					<xs:annotation>
						<xs:documentation>Description of all tabs of the framework.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element maxOccurs="unbounded" name="Tab" type="Tab">
								<xs:annotation>
									<xs:documentation>Describes a task of the framework.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
<!-- 
				<xs:element name="TaskHierarchy" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Description of the task hierarchy of the framework.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Task" type="TaskHierarchyDesc" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Describes a task in the task hierarchy of the framework.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
		-->
				<xs:element minOccurs="0" name="GlobalSteeringParameters">
					<xs:annotation>
						<xs:documentation>Description of the global steering parameters of the framework.</xs:documentation>
						<xs:documentation>Description of the global steering parameters of the framework.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element maxOccurs="unbounded" minOccurs="0" name="SteeringParameterField" type="SteeringParameterFieldDesc">
								<xs:annotation>
									<xs:documentation>Describes a global steering parameter field of the framework.</xs:documentation>
