    <Paths>
      <InputFilePath>./</InputFilePath>
      <OutputFilePath>./</OutputFilePath>
    </Paths>

    <HistogramRead>
      <Read>false</Read>
      <RunNumber>0</RunNumber>
    </HistogramRead>

    <Tasks>
      <Task>
        <TaskName>PrintFolders</TaskName>
