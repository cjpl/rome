<ROMEFrameworkDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="c:/rome/rome.xsd">
	<Experiment>
		<ProgramName>Example</ProgramName>
		<ExperimentShortCut>TH</ExperimentShortCut>
		<ProgramDefinitionVersion>1</ProgramDefinitionVersion>
	</Experiment>
	<Tasks>
		<Task>
			<TaskName>TaskA</TaskName>
		</Task>
		<Task>
			<TaskName>TaskA_A</TaskName>
		</Task>
		<Task>
			<TaskName>TaskB</TaskName>
			<Dependence>Task(TaskA)</Dependence>
		</Task>
		<Task>
			<TaskName>TaskC</TaskName>
			<Dependence>Task(TaskA) &amp;&amp; !Task(TaskB)</Dependence>
		</Task>
		<Task>
			<TaskName>TaskD</TaskName>
			<Dependence>!Task(TaskA_A) &amp;&amp; Tab(TabC)</Dependence>
		</Task>
	</Tasks>
	<Tabs>
		<Tab>
			<TabName>TabA</TabName>
			<Dependence>Task(TaskA)</Dependence>
		</Tab>
		<Tab>
			<TabName>TabB</TabName>
			<Dependence>Tab(TabA)</Dependence>
			<Tab>
				<TabName>TabC</TabName>
			</Tab>
		</Tab>
	</Tabs>
	<TaskHierarchy>
		<Task>
			<TaskName>TaskA</TaskName>
			<Task>
				<TaskName>TaskA_A</TaskName>
			</Task>
		</Task>
		<Task>
			<TaskName>TaskB</TaskName>
		</Task>
		<Task>
			<TaskName>TaskC</TaskName>
		</Task>
		<Task>
			<TaskName>TaskD</TaskName>
		</Task>
	</TaskHierarchy>
</ROMEFrameworkDefinition>
