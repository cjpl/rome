<ROMEFrameworkDefinition>
	<Experiment>
		<ExperimentName>Folders</ExperimentName>
		<ExperimentShortCut>FO</ExperimentShortCut>
		<ProgramName>monitor</ProgramName>
		<FrameworkDescription>Several kind of folders.</FrameworkDescription>
	</Experiment>
	<Author>
		<AuthorName>Ryu Sawada</AuthorName>
		<AuthorInstitute>ICEPP, University of Tokyo</AuthorInstitute>
		<AuthorCollaboration>Mu-E-Gamma (MEG)</AuthorCollaboration>
		<AuthorEmail>ryu.sawada _AtMark_ psi.ch</AuthorEmail>
	</Author>
	<Tabs>
		<Tab>
			<TabName>Tab</TabName>
			<TabTitle>1</TabTitle>
			<Author>Ryu Sawada</Author>
			<TabVersion>1</TabVersion>
		</Tab>
	</Tabs>
	<Folders>
		<Folder>
			<FolderName>FOData</FolderName>
			<FolderTitle>FOData</FolderTitle>
			<Author>Ryu Sawada</Author>
			<FolderVersion>1</FolderVersion>
			<FolderDescription>FO root folder.</FolderDescription>
			<Folder>
				<FolderName>ODB</FolderName>
				<FolderTitle>This folder reads ODB</FolderTitle>
				<DataBaseAccess>true</DataBaseAccess>
				<Author>Ryu Sawada</Author>
				<Field>
					<FieldName>RunNumber</FieldName>
					<FieldType>Int_t</FieldType>
					<FieldComment>Run number</FieldComment>
					<FieldInitialization>0</FieldInitialization>
<!-- ODB access is not supported yet -->
<!--
					<DataBaseName>myodb</DataBaseName>
					<DataBasePath>&quot;/Runinfo/Run number&quot;</DataBasePath>
-->
				</Field>
			</Folder>
			<Folder>
				<FolderName>ROMEDataBase</FolderName>
				<FolderTitle>This folder reads rome database</FolderTitle>
				<DataBaseAccess>true</DataBaseAccess>
				<Author>Ryu Sawada</Author>
				<Field>
					<FieldName>comment</FieldName>
					<FieldType>TString</FieldType>
					<FieldComment>Run number</FieldComment>
					<DataBaseName>myxml</DataBaseName>
					<DataBasePath>"/RunCatalog[id=%d]/comment",gAnalyzer->GetODB()->GetRunNumber()</DataBasePath>
				</Field>
				<Field>
					<FieldName>multipleDimension</FieldName>
					<FieldType>Float_t</FieldType>
					<FieldComment>multiple dimension field</FieldComment>
					<ArraySize>3:4:5</ArraySize>
				</Field>
				<Field>
					<FieldName>TArrayType</FieldName>
					<FieldType>TArrayI</FieldType>
					<FieldComment></FieldComment>
				</Field>
			</Folder>
			<Folder>
				<FolderName>VariousTypeOfFields</FolderName>
				<FolderTitle>This folder has various type of fields</FolderTitle>
				<Author>Ryu Sawada</Author>
				<Include>
					<IncludeName>TRef.h</IncludeName>
					<IncludeType>global</IncludeType>
				</Include>
				<Include>
					<IncludeName>TString.h</IncludeName>
					<IncludeType>global</IncludeType>
				</Include>
				<Include>
					<IncludeName>TVector3.h</IncludeName>
					<IncludeType>global</IncludeType>
				</Include>
				<!-- single fields -->
				<Field>
					<FieldName>SingleFolder</FieldName>
					<FieldType>DummyFolder</FieldType>
				</Field>
				<Field>
					<FieldName>SingleTRef</FieldName>
					<FieldType>TRef</FieldType>
					<InheritTObject>true</InheritTObject>
				</Field>
				<Field>
					<FieldName>SingleTArray</FieldName>
					<FieldType>TArrayI</FieldType>
				</Field>
				<Field>
					<FieldName>SingleTObjectObject</FieldName>
					<FieldType>TVector3</FieldType>
					<InheritTObject>true</InheritTObject>
				</Field>
				<Field>
					<FieldName>SingleTObjectPointer</FieldName>
					<FieldType>TVector3*</FieldType>
					<InheritTObject>true</InheritTObject>
				</Field>
				<Field>
					<FieldName>SingleSTDString</FieldName>
					<FieldType>std::string</FieldType>
				</Field>
				<Field>
					<FieldName>SingleTString</FieldName>
					<FieldType>TString</FieldType>
				</Field>
				<Field>
					<FieldName>SinglePremitive</FieldName>
					<FieldType>Double_t</FieldType>
				</Field>
				<!-- one dimension fixed length array fields -->
				<Field>
					<FieldName>OneDimFixedFolder</FieldName>
					<FieldType>DummyFolder</FieldType>
					<ArraySize>10</ArraySize>
				</Field>
				<Field>
					<FieldName>OneDimFixedTRef</FieldName>
					<FieldType>TRef</FieldType>
					<InheritTObject>true</InheritTObject>
					<ArraySize>10</ArraySize>
				</Field>
				<Field>
					<FieldName>OneDimFixedTArray</FieldName>
					<FieldType>TArrayI</FieldType>
					<ArraySize>10</ArraySize>
				</Field>
				<Field>
					<FieldName>OneDimFixedTObjectObject</FieldName>
					<FieldType>TVector3</FieldType>
					<InheritTObject>true</InheritTObject>
					<ArraySize>10</ArraySize>
				</Field>
				<Field>
					<FieldName>OneDimFixedTObjectPointer</FieldName>
					<FieldType>TVector3*</FieldType>
					<InheritTObject>true</InheritTObject>
					<ArraySize>10</ArraySize>
				</Field>
				<Field>
					<FieldName>OneDimFixedSTDString</FieldName>
					<FieldType>std::string</FieldType>
					<ArraySize>10</ArraySize>
				</Field>
				<Field>
					<FieldName>OneDimFixedTString</FieldName>
					<FieldType>TString</FieldType>
					<ArraySize>10</ArraySize>
				</Field>
				<Field>
					<FieldName>OneDimFixedPremitive</FieldName>
					<FieldType>Double_t</FieldType>
					<ArraySize>10</ArraySize>
				</Field>
				<!-- one dimension variable length array fields -->
				<Field>
					<FieldName>OneDimVariableFolder</FieldName>
					<FieldType>DummyFolder</FieldType>
					<ArraySize>variable</ArraySize>
					<FieldComment>!</FieldComment>
				</Field>
				<Field>
					<FieldName>OneDimVariableTRef</FieldName>
					<FieldType>TRef</FieldType>
					<InheritTObject>true</InheritTObject>
					<ArraySize>variable</ArraySize>
					<FieldComment>!</FieldComment>
				</Field>
<!--
				<Field>
					<FieldName>OneDimVariableTArray</FieldName>
					<FieldType>TArrayI</FieldType>
					<ArraySize>variable</ArraySize>
					<FieldComment>!</FieldComment>
				</Field>
-->
				<Field>
					<FieldName>OneDimVariableTObjectObject</FieldName>
					<FieldType>TVector3</FieldType>
					<InheritTObject>true</InheritTObject>
					<ArraySize>variable</ArraySize>
					<FieldComment>!</FieldComment>
				</Field>
				<Field>
					<FieldName>OneDimVariableTObjectPointer</FieldName>
					<FieldType>TVector3*</FieldType>
					<InheritTObject>true</InheritTObject>
					<ArraySize>variable</ArraySize>
					<FieldComment>!</FieldComment>
				</Field>
				<Field>
					<FieldName>OneDimVariableSTDString</FieldName>
					<FieldType>std::string</FieldType>
					<ArraySize>variable</ArraySize>
					<FieldComment>!</FieldComment>
				</Field>
				<Field>
					<FieldName>OneDimVariableTString</FieldName>
					<FieldType>TString</FieldType>
					<ArraySize>variable</ArraySize>
					<FieldComment>!</FieldComment>
				</Field>
				<Field>
					<FieldName>OneDimVariablePremitive</FieldName>
					<FieldType>Double_t</FieldType>
					<ArraySize>variable</ArraySize>
					<FieldComment>!</FieldComment>
				</Field>
				<!-- multi dimension fixed length array fields -->
<!--
				<Field>
					<FieldName>MultiDimFolder</FieldName>
					<FieldType>DummyFolder</FieldType>
					<ArraySize>10:4:2</ArraySize>
				</Field>
-->
<!--
				<Field>
					<FieldName>MultiDimTRef</FieldName>
					<FieldType>TRef</FieldType>
					<InheritTObject>true</InheritTObject>
					<ArraySize>10:4:2</ArraySize>
				</Field>
-->
<!--
				<Field>
					<FieldName>MultiDimTArray</FieldName>
					<FieldType>TArrayI</FieldType>
					<ArraySize>10:4:2</ArraySize>
				</Field>
-->
<!--
				<Field>
					<FieldName>MultiDimTObjectObject</FieldName>
					<FieldType>TVector3</FieldType>
					<InheritTObject>true</InheritTObject>
					<ArraySize>10:4:2</ArraySize>
				</Field>
-->
<!--
				<Field>
					<FieldName>MultiDimTObjectPointer</FieldName>
					<FieldType>TVector3*</FieldType>
					<InheritTObject>true</InheritTObject>
					<ArraySize>10:4:2</ArraySize>
				</Field>
-->
<!--
				<Field>
					<FieldName>MultiDimSTDString</FieldName>
					<FieldType>std::string</FieldType>
					<ArraySize>10:4:2</ArraySize>
				</Field>
-->
<!--
				<Field>
					<FieldName>MultiDimTString</FieldName>
					<FieldType>TString</FieldType>
					<ArraySize>10:4:2</ArraySize>
				</Field>
-->
				<Field>
					<FieldName>MultiDimPremitive</FieldName>
					<FieldType>Double_t</FieldType>
					<ArraySize>10:4:2</ArraySize>
				</Field>
			</Folder>
		</Folder>
	</Folders>
	<SupportFolders>
			<SupportFolder>
				<FolderName>DummyFolder</FolderName>
				<FolderTitle>Dummy</FolderTitle>
				<Author>Ryu Sawada</Author>
				<Field>
					<FieldName>DummyField</FieldName>
					<FieldType>Int_t</FieldType>
				</Field>
			</SupportFolder>
        </SupportFolders>
	<NetFolders>
		<NetFolder>
			<NetFolderName>mynetfolder</NetFolderName>
			<NetFolderTitle>rome folder</NetFolderTitle>
			<NetFolderRoot>FO</NetFolderRoot>
			<NetFolderHost>localhost</NetFolderHost>
			<NetFolderPort>9090</NetFolderPort>
		</NetFolder>
	</NetFolders>
	<UserMakeFile>
		<DictionaryHeaders>
			<Header>
				<HeaderName>romeproject/include/generated/FORunInfo.h</HeaderName>
			</Header>
			<Header>
				<HeaderName>romeproject/include/generated/FOPMTData.h</HeaderName>
			</Header>
		</DictionaryHeaders>
		<IncludeDirectories>
			<IncludeDirectory>romeproject/include</IncludeDirectory>
		</IncludeDirectories>
		<AdditionalFiles>
			<File>
				<SourceFile>romeproject/src/generated/FORunInfo.cpp</SourceFile>
				<HeaderFile>romeproject/include/generated/FORunInfo.h</HeaderFile>
			</File>
			<File>
				<SourceFile>romeproject/src/generated/FOPMTData.cpp</SourceFile>
				<HeaderFile>romeproject/include/generated/FOPMTData.h</HeaderFile>
			</File>
		</AdditionalFiles>
	</UserMakeFile>
</ROMEFrameworkDefinition>
