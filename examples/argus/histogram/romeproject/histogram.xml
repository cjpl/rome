<?xml version="1.0" encoding="ISO-8859-1"?>
<ROMEFrameworkDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="c:/rome/rome/rome.xsd">
  <Experiment>
    <ExperimentName>Histograms</ExperimentName>
    <ExperimentShortCut>HG</ExperimentShortCut>
    <ProgramName>framework</ProgramName>
    <FrameworkDescription>Example to show histograms.</FrameworkDescription>
  </Experiment>
  <Author>
    <AuthorName>Ryu Sawada</AuthorName>
    <AuthorInstitute>ICEPP, University of Tokyo</AuthorInstitute>
    <AuthorCollaboration>Mu-E-Gamma (MEG)</AuthorCollaboration>
    <AuthorEmail>ryu.sawada _AtMark_ psi.ch</AuthorEmail>
  </Author>
  <Tasks>
    <Task>
      <TaskName>MakeHistogram</TaskName>
      <TaskVersion>1</TaskVersion>
      <TaskDescription>Create and fill histograms at the begin of run</TaskDescription>
      <Histogram>
        <HistName>hAdc</HistName>
        <HistTitle>ADC</HistTitle>
        <HistFolderName>ADC</HistFolderName>
        <HistFolderTitle>ADC</HistFolderTitle>
        <HistType>TH1F</HistType>
        <HistXNbins>3996</HistXNbins>
        <HistXmin>10</HistXmin>
        <HistXmax>3996</HistXmax>
      </Histogram>
    </Task>
  </Tasks>
  <UserDAQSystems>
    <UserDAQSystem>
      <DAQSystemName>dummy</DAQSystemName>
    </UserDAQSystem>
  </UserDAQSystems>
</ROMEFrameworkDefinition>
