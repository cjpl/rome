<ROMEFrameworkDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="c:/rome/rome.xsd">
	<Experiment>
		<ExperimentShortCut>DO</ExperimentShortCut>
		<ProgramName>Example</ProgramName>
	</Experiment>
	<Tasks>
		<Task>
			<TaskName>FillHisto</TaskName>
			<Histogram>
				<HistName>MyHisto</HistName>
				<HistTitle>MyHisto</HistTitle>
				<HistFolderName>myhistos</HistFolderName>
				<HistType>TH1F</HistType>
				<HistArraySize>3</HistArraySize>
				<HistXLabel>X</HistXLabel>
				<HistYLabel>Entries</HistYLabel>
				<HistXNbins>100</HistXNbins>
				<HistXmin>-100</HistXmin>
				<HistXmax>100</HistXmax>
			</Histogram>
			<Histogram>
				<HistName>MyOtherHisto</HistName>
				<HistTitle>MyOtherHisto</HistTitle>
				<HistFolderName>myotherhistos</HistFolderName>
				<HistType>TH1F</HistType>
				<HistArraySize>3</HistArraySize>
				<HistXLabel>X</HistXLabel>
				<HistYLabel>Entries</HistYLabel>
				<HistXNbins>100</HistXNbins>
				<HistXmin>-100</HistXmin>
				<HistXmax>100</HistXmax>
			</Histogram>
			<Histogram>
				<HistName>My2DHisto</HistName>
				<HistTitle>MyOtherHisto</HistTitle>
				<HistFolderName>myotherhistos</HistFolderName>
				<HistType>TH2F</HistType>
				<HistArraySize>3</HistArraySize>
				<HistXLabel>X</HistXLabel>
				<HistYLabel>Entries</HistYLabel>
				<HistXNbins>100</HistXNbins>
				<HistXmin>-100</HistXmin>
				<HistXmax>100</HistXmax>
				<HistYNbins>100</HistYNbins>
				<HistYmin>-100</HistYmin>
				<HistYmax>100</HistYmax>
			</Histogram>
		</Task>
	</Tasks>
	<Tabs>
		<Tab>
			<TabName>Main</TabName>
			<TabTitle>Main</TabTitle>
			<Tab>
				<TabName>Histos1D</TabName>
				<TabTitle>Histos 1D</TabTitle>
				<HistogramDisplay>true</HistogramDisplay>
				<DisplayObjects>
					<DisplayObject>
						<Object>MyHisto</Object>
						<ObjectTitle>MyHisto</ObjectTitle>
					</DisplayObject>
					<DisplayObject>
						<Object>MyOtherHisto</Object>
						<ObjectTitle>MyOtherHisto</ObjectTitle>
					</DisplayObject>
				</DisplayObjects>
			</Tab>
			<Tab>
				<TabName>Histos2D</TabName>
				<TabTitle>Histos 2D</TabTitle>
				<HistogramDisplay>true</HistogramDisplay>
				<DisplayObjects>
					<DisplayObject>
						<Object>My2DHisto</Object>
						<ObjectTitle>My2DHisto</ObjectTitle>
					</DisplayObject>
				</DisplayObjects>
			</Tab>
		</Tab>
	</Tabs>
</ROMEFrameworkDefinition>
