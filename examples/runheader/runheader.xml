<ROMEFrameworkDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="c:/rome/rome.xsd">
	<Experiment>
		<ExperimentShortCut>RUNH</ExperimentShortCut>
		<ProgramName>Analyzer</ProgramName>
	</Experiment>
	<Folders>
		<Folder>
			<FolderName>RunInfo</FolderName>
			<Field>
				<FieldName>TriggerType</FieldName>
				<FieldType>Int_t</FieldType>
				<FieldComment>Type of trigger of the run</FieldComment>
			</Field>
		</Folder>
		<Folder>
			<FolderName>Calibration</FolderName>
			<ArraySize>10</ArraySize>
			<Field>
				<FieldName>Pedestal</FieldName>
				<FieldType>Int_t</FieldType>
				<FieldComment>Calibratin parameter of ADC</FieldComment>
			</Field>
		</Folder>
		<Folder>
			<FolderName>EventData</FolderName>
			<Field>
				<FieldName>ADC</FieldName>
				<FieldType>Int_t</FieldType>
				<FieldComment>ADC Value</FieldComment>
				<ArraySize>10</ArraySize>
			</Field>
		</Folder>
	</Folders>
	<Tasks>
		<Task>
			<TaskName>Task1</TaskName>
		</Task>
	</Tasks>
	<Trees>
		<Tree>
			<TreeName>DataTree</TreeName>
			<RunHeader>
				<RunHeaderName>runinfo</RunHeaderName>
				<RelatedFolder>RunInfo</RelatedFolder>
			</RunHeader>
			<RunHeader>
				<RunHeaderName>calib</RunHeaderName>
				<RelatedFolder>Calibration</RelatedFolder>
			</RunHeader>
			<Branch>
				<BranchName>data</BranchName>
				<RelatedFolder>EventData</RelatedFolder>
			</Branch>
		</Tree>
	</Trees>
	<GlobalSteeringParameters>
		<SteeringParameterField>
			<SPFieldName>TriggerType</SPFieldName>
			<SPFieldType>Int_t</SPFieldType>
		</SteeringParameterField>
	</GlobalSteeringParameters>
</ROMEFrameworkDefinition>
