<ROMEFrameworkDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="c:/rome/rome.xsd">
	<Experiment>
		<ExperimentShortCut>TCUTG</ExperimentShortCut>
		<ProgramName>Example</ProgramName>
	</Experiment>
	<Tasks>
		<Task>
			<TaskName>FillHisto</TaskName>
			<Histogram>
				<HistName>MyHisto</HistName>
				<HistFolderName>myhistos</HistFolderName>
				<HistType>TH1F</HistType>
				<HistXLabel>X</HistXLabel>
				<HistYLabel>Entries</HistYLabel>
				<HistXNbins>100</HistXNbins>
				<HistXmin>-100</HistXmin>
				<HistXmax>100</HistXmax>
				<Argus>
					<Tab>
						<TabName>GeneratedHisto</TabName>
						<HistArrayIndex>0</HistArrayIndex>
						<Index>0</Index>
					</Tab>
				</Argus>
			</Histogram>
			<Graph>
				<GraphName>MyCutG</GraphName>
				<GraphFolderName>mygraphs</GraphFolderName>
				<GraphType>TCutG</GraphType>
				<GraphXLabel>X</GraphXLabel>
				<GraphYLabel>Y</GraphYLabel>
				<Argus>
					<Tab>
						<TabName>GeneratedHisto</TabName>
						<Index>0</Index>
					</Tab>
				</Argus>
			</Graph>
		</Task>
	</Tasks>
	<Tabs>
		<Tab>
			<TabName>Histos</TabName>
			<TabTitle>Histos</TabTitle>
			<TabObjectDisplay>true</TabObjectDisplay>
			<ObjectDisplays>
				<ObjectDisplay>
					<ObjectDisplayName>MyHisto</ObjectDisplayName>
					<ObjectDisplayTitle>MyHisto</ObjectDisplayTitle>
					<ObjectDisplayObject>MyHisto</ObjectDisplayObject>
					<ObjectDisplayObject>MyCutG</ObjectDisplayObject>
				</ObjectDisplay>
			</ObjectDisplays>
		</Tab>
	</Tabs>
</ROMEFrameworkDefinition>
